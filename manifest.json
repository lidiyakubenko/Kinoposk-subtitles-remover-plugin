{
  "manifest_version": 2,
  "name": "Kinopoisk subtitles remover",
  "description": "Убирает принудительные субтитры в кинопоиске",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}